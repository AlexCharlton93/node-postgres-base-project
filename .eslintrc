{
    "env": {
        "jest/globals": true
    },
    "extends": ["airbnb", "plugin:security/recommended"],
    "plugins": ["security", "jest"],
    "rules": {
        "linebreak-style": 0,
        "import/named": 0,
        "object-curly-newline": 0,
        "arrow-body-style": "off",
        "curly": ["error", "all"],
        "class-methods-use-this": "off",
        "padding-line-between-statements": [
            "error",
            {
                "blankLine": "always",
                "next": "return",
                "prev": "*"
            },
            {
                "blankLine": "always",
                "next": "*",
                "prev": ["const", "let", "var"]
            },
            {
                "blankLine": "always",
                "next": ["const", "let", "var"],
                "prev": "*"
            },
            {
                "blankLine": "any",
                "next": ["const", "let", "var"],
                "prev": ["const", "let", "var"]
            }
        ],
        "import/prefer-default-export": "off",
        "indent": ["error", "tab"],
        "max-len": ["error", 200],
        "no-underscore-dangle": "off",
        "no-tabs": 0,
        "import/export": 0
    }
}
